{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev:env": "docker-compose --env-file .env.dev -f docker-compose.yaml up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "dotenv -e .env.dev -- prisma generate",
    "prisma:migrate": "dotenv -e .env.dev -- prisma migrate dev --preview-feature",
    "prisma:studio": "dotenv -e .env.dev -- prisma studio"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.9.5",
    "@nestjs/platform-express": "^7.0.0",
    "@prisma/client": "2.15.0",
    "@types/bcrypt": "^3.0.0",
    "@types/graphql-fields": "^1.3.3",
    "apollo-server-express": "^2.19.2",
    "bcrypt": "^5.0.0",
    "graphql": "^15.4.0",
    "graphql-fields": "^2.0.3",
    "graphql-tools": "^7.0.2",
    "jsonwebtoken": "^8.5.1",
    "nest-access-control": "^2.0.2",
    "nestjs-redis": "^1.2.8",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@prisma-generators/nestjs": "^0.6.0-alpha5",
    "@prisma/cli": "2.15.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "prisma-nestjs-graphql": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typegraphql-prisma": "^0.10.0",
    "typegraphql-prisma-nestjs": "^0.600.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
